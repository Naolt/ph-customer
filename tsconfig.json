{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx",
    "app/index.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(auth)/login.tsx",
    "app/(auth)/register.jsx"
  ]
}
